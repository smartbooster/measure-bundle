{
  "name": "smartbooster/measure-bundle",
  "type": "symfony-bundle",
  "description": "SMART BOOSTER Measure bundle",
  "keywords": [],
  "homepage": "https://github.com/smartbooster/etl-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Nicolas Bastien",
      "email": "nicolas.bastien@smartbooster.io",
      "homepage": "https://smartbooster.io"
    }
  ],
  "require": {
    "doctrine/doctrine-bundle": "^1.6",
    "doctrine/orm": "^2.5",
    "symfony/http-kernel": "^2.8 || ^3.2 || ^4.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0",
    "phpunit/phpunit": "6.*",
    "liip/functional-test-bundle": "^1.9.5",
    "nelmio/alice": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "Smart\\MeasureBundle\\": "src/"
    }
